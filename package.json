{
  "name": "hook-di",
  "version": "0.0.4",
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "test": "vitest"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.iife.js",
  "engines" : { "node" : ">= 12.7.0" },
  "exports": {
    ".": {
      "browser": "./dist/index.esm-browser.js",
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.mjs"
    },
    "./vue": {
      "browser": "./dist/vue.esm-browser.js",
      "node": {
        "import": "./dist/vue.mjs",
        "require": "./dist/vue.cjs"
      },
      "import": "./dist/vue.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": ["dist/*"]
    }
  },
  "repository": "https://github.com/cj97",
  "keywords": [
    "typescript",
    "hook",
    "di",
    "ioc"
  ],
  "description": "hook style di implement",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^17.0.23",
    "happy-dom": "^2.55.0",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.3.0",
    "vitest": "^0.9.2",
    "vue": "^3.2.31"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}